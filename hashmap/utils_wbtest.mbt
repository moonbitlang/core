test "capacity" {
  let m : HashMap[String, Int] = HashMap::new()
  @test.eq(m.capacity(), 8)!
  m.set("a", 1)
  m.set("b", 2)
  m.set("c", 3)
  m.set("d", 4)
  m.set("e", 5)
  m.set("f", 6)
  m.set("g", 7)
  m.set("h", 8)
  @test.eq(m.capacity(), 16)!
}

  new[T]() -> Self[T]
  op_add[T](Self[T], Self[T]) -> Self[T]
  op_as_view[T](Self[T], ~start : Int, ~end : Int) -> ArrayView[T]
  op_equal[T : Eq](Self[T], Self[T]) -> Bool

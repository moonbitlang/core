  // Starts with a letter
  @json.inspect(parse("(?<group>abc)"), content={
    "ast": [
      "Capture 1",
      [
        ["CharClass neg=false", ["'a'", "'a'"]],
        ["CharClass neg=false", ["'b'", "'b'"]],
